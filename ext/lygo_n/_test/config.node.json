{
  "workspace": "./_workspace",
  "log_level": "warn",
  "discovery": {
    "network_id": "net1",
    "publishers": [],
    "publisher": {
      "enabled": true
    },
    "publish": {
      "enabled": true,
      "address": "localhost:10001"
    },
    "network": {
      "enabled": true
    }
  },
  "client": {
    "enabled": true,
    "nio": {
      "address": "localhost:10001"
    }
  },
  "server": {
    "enabled": true,
    "http": {
      "websocket_endpoint": "/ws",
      "hosts": [
        {
          "addr": ":80",
          "tls": false,
          "websocket": {
            "enabled": true
          }
        },
        {
          "addr": ":443",
          "tls": true,
          "ssl_cert": "./cert/ssl-cert.pem",
          "ssl_key": "./cert/ssl-cert.key",
          "websocket": {
            "enabled": true
          }
        }
      ],
      "enable_request_id": true,
      "prefork": false,
      "static": [
        {
          "enabled": true,
          "prefix": "/",
          "root": "./www",
          "index": "",
          "compress": true
        }
      ],
      "compression": {
        "enabled": false,
        "level": 0
      },
      "limiter": {
        "enabled": false,
        "timeout": 30,
        "max": 10
      }
    },
    "nio": {
      "address": ":10001"
    }
  }
}